<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net462</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="XrmFramework" Version="#{GitBuildVersionSimple}#" />
    <PackageReference Include="XrmFramework.Plugin" Version="#{GitBuildVersionSimple}#" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="..\$(XrmFrameworkCoreProjectName)\\**\\*.cs" Exclude="..\$(XrmFrameworkCoreProjectName)\obj\**;..\$(XrmFrameworkCoreProjectName)\bin\**" Link="__Imports\$(XrmFrameworkCoreProjectName)\%(RecursiveDir)%(Filename)%(Extension)" />
  </ItemGroup>
  
</Project>
